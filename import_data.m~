% function to import data for a given country

function data = import_data(country)
    
    temp = readtable('data.csv');

    data = temp('location'='USA');
    
    num_rows = height(temp);
    
    first_row = 0;
    last_row = 0;
    
    for row = 1:num_rows
        
        single_row = temp(row,'location')(3);
        if single_row = country
            if first_row = 0
               first_row = row;
  
            end
        end
        
    end
end